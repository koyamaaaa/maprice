<!DOCTYPE html>
<html>
  <head>
    <title>App</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <a href="/">トップ</a>
    <% if request.path != '/admin_menu' %>
      <%= link_to '管理画面トップ', dashbords_path %>
    <% end %>
    <% if logged_in? %>
      <%= link_to 'ログアウト', logout_path, method: :delete %>
    <% else %>
      <%= link_to 'ログイン', login_path %>
    <% end %>
    <%= link_to '管理画面トップ', controller: 'dashbords', action: 'index' %>
    <%= link_to 'プラン一覧', plans_path %>
    <%= link_to 'キャンペーン一覧', campaigns_path %>
    <%= link_to '通信容量一覧' , data_capacities_path %>
    <%= link_to '端末一覧', devices_path %>
    <%= link_to '回線一覧' , net_lines_path %>
    <%= link_to 'サービス一覧', services_path %>
    <%= link_to 'プロバイダー一覧', providers_path %>
    <%= link_to 'ユーザー一覧', users_path %>
    <%= yield %>
  </body>
</html>