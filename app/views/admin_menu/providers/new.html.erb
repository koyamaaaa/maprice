<% flash[:alert] if flash[:alert] %>
<%= form_with model: @provider, method: :post, local: true do |f| %>
  <%= f.label :name, 'プロバイダ名' %>
  <%= f.text_field :name %>

  <%= f.label :service_id, 'サービス名' %>
  <%= f.collection_select :service_id, Service.all, :id, :name %>
  
  <%= f.submit %>
<% end %>