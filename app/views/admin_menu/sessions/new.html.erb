<h1>Sessions#new</h1>
<%= form_with model: @session, url: login_path do |f| %>
  <%= f.label :name , 'ユーザー名' %>
  <%= f.text_field :name %>

  <%= f.label :email, 'メールアドレス' %>
  <%= f.text_field :email %>

  <%= f.label :password, 'パスワード' %>
  <%= f.text_field :password %>

  <%= f.label :password_confirmation, '確認' %>
  <%= f.text_field :password_confirmation %>

  <%= f.submit 'login' %>

<% end %>